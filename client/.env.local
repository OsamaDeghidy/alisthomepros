# Backend API URL
# For development - uncomment this line for local development
#NEXT_PUBLIC_API_URL=http://127.0.0.1:8000/api
# For production testing - uncomment this line to test with production backend
NEXT_PUBLIC_API_URL=https://www.custrus.com/api

# App Configuration
NEXT_PUBLIC_APP_NAME=A-List
NEXT_PUBLIC_APP_VERSION=1.0.0

# File Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=10485760
NEXT_PUBLIC_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Map Configuration (if needed)
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Analytics (if needed)
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_id_here
