# Production Environment Variables
# Backend API URL for production
NEXT_PUBLIC_API_URL=https://www.custrus.com/api

# App Configuration
NEXT_PUBLIC_APP_NAME=A-List
NEXT_PUBLIC_APP_VERSION=1.0.0

# File Upload Configuration
NEXT_PUBLIC_MAX_FILE_SIZE=10485760
NEXT_PUBLIC_ALLOWED_FILE_TYPES=image/jpeg,image/png,image/gif,image/webp,application/pdf,application/msword,application/vnd.openxmlformats-officedocument.wordprocessingml.document

# Map Configuration
NEXT_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# Analytics
NEXT_PUBLIC_GOOGLE_ANALYTICS_ID=your_ga_id_here